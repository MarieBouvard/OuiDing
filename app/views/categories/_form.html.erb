<%= form_for @categorie do |f| %>
    <br/>
        <div class="field form-group">
            <%= f.text_field :name, autofocus: true, autocomplete: "name", placeholder: "Nom de catégorie", class:"formControl" %>
        <br/>
        </div>
        <br/>

        <div class="field form-group">
            <%= f.file_field :picture %>
        </div>
        <br/>

    
        <div class="actions">
            <%= f.submit "Rajouter cette catégorie", :class => 'btnbis' %>
        </div>
        
<% end %>
    
    
                